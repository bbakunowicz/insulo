    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^1.17.3",
    "supertest": "^4.0.2"
  }
